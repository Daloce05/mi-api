<!DOCTYPE html>
<html>
<head>
  <title>Lista de Celulares</title>
</head>
<body>
  <h1>Lista de Celulares</h1>
  <ul>
    <% celulares.forEach(celular => { %>
      <li>
        <strong>Nombre:</strong> <%= celular.nombre %> <br>
        <strong>Marca:</strong> <%= celular.marca %> <br>
        <strong>Color:</strong> <%= celular.color %> <br>
        <strong>Precio:</strong> $<%= celular.price %> <br>
        <strong>Almacenamiento:</strong> <%= celular.almacenamiento %> <br>
        <strong>Descripción:</strong> <%= celular.descripcion %> <br>
        <strong>Ubicación:</strong> <%= celular.ubicacion %> <br>
        <strong>Stock:</strong> <%= celular.stock %> <br>
        <strong>Imagen:</strong> <img src="<%= celular.image %>" alt="<%= celular.nombre %>"> <br>
      </li>
    <% }); %>
  </ul>

  <form action="/celulares" method="post">
    <label for="nombre">Nombre:</label>
    <input type="text" id="nombre" name="nombre" required><br><br>

    <label for="marca">Marca:</label>
    <input type="text" id="marca" name="marca" required><br><br>

    <label for="color">Color:</label>
    <input type="text" id="color" name="color" required><br><br>

    <label for="price">Precio:</label>
    <input type="text" id="price" name="price" required><br><br>

    <label for="almacenamiento">Almacenamiento:</label>
    <input type="text" id="almacenamiento" name="almacenamiento" required><br><br>

    <label for="descripcion">Descripción:</label>
    <textarea id="descripcion" name="descripcion"></textarea><br><br>

    <label for="ubicacion">Ubicación:</label>
    <textarea id="ubicacion" name="ubicacion"></textarea><br><br>

    <label for="stock">Stock:</label>
    <input type="number" id="stock" name="stock" required><br><br>

    <label for="image">URL de la Imagen:</label>
    <input type="text" id="image" name="image"><br><br>

    <button type="submit">Agregar Celular</button>
  </form>
</body>
</html>
